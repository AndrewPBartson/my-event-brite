
@model WebMvc.Models.Event




<form class="img_spacer" asp-controller="EventBrowser" asp-action="">
    <img class="esh-event-thumbnail event_image" src="@Model.EventImageUrl" />
    
    <div class="event_name">
        <span>@Model.Title</span>
    </div>
    <div class="event_intro">
        <span>@Model.Description</span>
    </div>
    <input type="hidden" asp-for="@Model.Address" name="Address" />
    <input type="hidden" asp-for="@Model.MaxOccupancy" name="MaxOccupancy" />
    <input type="hidden" asp-for="@Model.MaxTicketsPerUser" name="MaxTicketsPerUser" />
    <input type="hidden" asp-for="@Model.StartDate" name="StartDate" />
    <input type="hidden" asp-for="@Model.EndDate" name="EndDate" />
    <input type="hidden" asp-for="@Model.EventLinkUrl" name="EventLinkUrl" />
    
</form>